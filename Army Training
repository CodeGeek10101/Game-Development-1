# Spawn a hero and add a goal.
game.spawnPlayerXY("champion", 40, 15)
game.addDefeatGoal()

# Spawn at least 2 "munchkin"s.
game.spawnXY("munchkin", 36, 30)
game.spawnXY("munchkin", 44, 30)
# Spawn at least 2 "thrower"s.
game.spawnXY("thrower", 32, 30)
game.spawnXY("thrower", 30, 30)
# Spawn at least 2 "soldier"s.
game.spawnXY("soldier", 31, 30)
game.spawnXY("soldier", 30, 30)
# Spawn at least 2 "archer"s.
game.spawnXY("archer", 35, 30)
game.spawnXY("archer", 34, 30)
